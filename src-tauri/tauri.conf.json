{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Filey",
  "version": "0.1.0",
  "identifier": "wander.filey",
  "build": {
    "devUrl": "http://127.0.0.1:3000",
    "frontendDist": "../out",
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build"
  },
  "app": {
    "windows": [
      {
        "title": "Filey",
        "width": 411,
        "height": 710,
        "minWidth": 411,
        "minHeight": 710,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
